                                    } else if (Number(arry[9]) < 0) {
                                        $("#zdf").text(arry[9] + "%").attr("class", "span_green");
                                    } else {
                                        $("#zdf").text(arry[9] + "%").attr("class", "");
                                    }

                                    $("#zf").text(arry[14] + "%").attr("class", "");
                                    $("#cjl").text(formatNumber(arry[10], 2)).attr("class", "");

                                    if (GetQueryString("color") == "w") {
                                        $("#cje").text(formatNumber(arry[11], 2)).css("color", "#0079D5");
                                    } else {
                                        $("#cje").text(formatNumber(arry[11], 2));
                                    }
                                   
                                    //if (Number(arry[6]) > 0) {
                                    //    $("#zxj").text(arry[4]).attr("class", "span_red");
                                    //    $("#zd").text(arry[6]).attr("class", "span_red");
                                    //    $("#zdf").text(arry[5] + "%").attr("class", "span_red");
