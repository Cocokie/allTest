                    <td style="text-align: center; width: 74px; ">{{value.EDUCATION}}</td>
                    <td style="text-align: center; width: 124px; ">{{value.BIRTHDATE}}</td>
                    <td style="text-align: center; width: 345px; ">{{value.OCCUPATION}}</td>
                </tr>
                <tr class="detailinfo" style="display:none">
                    <td style="width:70px">&nbsp;</td>
                    <td colspan="5" style="text-align:left">
                        <ul id="dataType" class="common-tab-list">
                            <li onclick="jlClick(this,1)" class="active">
                                <a href="javascript:;">中文简历</a>
                            </li>
                            <li onclick="jlClick(this,2)" class="no-margin">
                                <a href="javascript:;">英文简历</a>
                            </li>
                        </ul>
                        <div style="display:block">
                            
                            <div style="TEXT-INDENT:2em;" class="p_div">
                                {{formatProfile(value.RESUME,1)}}
                            </div>
                        </div>
                        <div style="display:none">
                            
                            <div style="TEXT-INDENT:2em;" class="p_div">
                                {{formatProfile(value.RESUMEEN,2)}}
                            </div>
                        </div>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    {{/if}}
</script>


