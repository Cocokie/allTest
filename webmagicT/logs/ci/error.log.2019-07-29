                var url = "https://nufm.dfcfw.com/EM_Finance2014NumericApplication/JS.aspx?type=CT&cmd=" + cmd + "7&sty=FC2DPFD&st=z&sr=&p=&ps=&cb=?&token=07ff5989be84610cf22f1c1020880b90";
